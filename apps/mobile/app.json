{
  "expo": {
    "name": "Habibi Stream",
    "slug": "habibi-stream",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "habibi-stream",
    "splash": {
      "backgroundColor": "#09090b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.habibi.stream"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#09090b"
      },
      "package": "com.habibi.stream"
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "plugins": ["expo-font", "expo-router"]
  }
}
